{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    },
    {
      "pattern": "^mailto:"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://github.com/glaucus03/task-github-search/blob/main/"
    }
  ],
  "httpStatusCodesToIgnore": [429, 503],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackHTTPStatus": 200
}